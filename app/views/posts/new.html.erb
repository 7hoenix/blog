<h1>I'm where you go to enter you post information! yayyy</h1>

<%= form_for @post do |f| %>
    <%= f.label :Title %>
    <%= f.text_area :title %>
    <%= f.label :Author %>
    <%= f.text_area :author %>
    <%= f.label :Content %>
    <%= f.text_area :content %>
    <%= f.submit :Post %>
<% end %>